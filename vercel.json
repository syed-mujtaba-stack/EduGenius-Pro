{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": "client/.next"
      }
    }
  ],
  "functions": {
    "client/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "TURBOPACK": "0",
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "TURBOPACK": "0",
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production"
    }
  }
}
